{
  "name": "vscode-expo",
  "displayName": "Expo Tools",
  "version": "0.2.0",
  "description": "Expo tools for vscode",
  "keywords": [
    "expo",
    "vscode"
  ],
  "license": "MIT",
  "author": {
    "email": "me@bycedric.com",
    "name": "Cedric van Putten",
    "url": "https://bycedric.com"
  },
  "publisher": "byCedric",
  "icon": "images/logo-marketplace.png",
  "homepage": "https://github.com/expo-community/vscode-expo",
  "bugs": {
    "url": "https://github.com/expo-community/vscode-expo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo-community/vscode-expo"
  },
  "main": "./out/src/extension.js",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "vscode:prepublish": "yarn clean && yarn build:production",
    "start": "yarn build --watch",
    "build": "tsc -p ./",
    "build:production": "esbuild ./src/extension.ts --bundle --minify --sourcemap --platform=node --external:vscode --outfile=./out/src/extension.js",
    "clean": "git clean -xdf ./out ./.vscode-test *tsbuildinfo",
    "test": "node ./out/test/jest/cli.js",
    "lint": "eslint .",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@expo/babel-preset-cli": "^0.2.20",
    "@expo/config-plugins": "~1.0.29",
    "@expo/xdl": "^59.1.0",
    "@types/jest": "^26.0.10",
    "@types/json-schema-traverse": "^0.4.0",
    "@types/node": "^14.6.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "esbuild": "^0.11.20",
    "eslint": "^7.7.0",
    "eslint-config-universe": "^4.0.0",
    "find-up": "^5.0.0",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "jest-environment-node": "^26.3.0",
    "json-schema-traverse": "^0.4.1",
    "jsonc-parser": "^3.0.0",
    "prettier": "^2.1.1",
    "typescript": "^3.8.3",
    "vsce": "^1.79.3",
    "vscode-test": "^1.4.0"
  }
}
