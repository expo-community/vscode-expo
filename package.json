{
	"name": "vscode-expo",
	"displayName": "Expo Tools",
	"version": "0.0.3",
	"description": "Expo tools for vscode",
	"keywords": [
		"expo",
		"vscode"
	],
	"license": "MIT",
	"author": {
		"email": "me@bycedric.com",
		"name": "Cedric van Putten",
		"url": "https://bycedric.com"
	},
	"publisher": "byCedric",
	"icon": "images/icon.png",
	"homepage": "https://github.com/expo-community/vscode-expo",
	"bugs": {
		"url": "https://github.com/expo-community/vscode-expo/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/expo-community/vscode-expo"
	},
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [
		"*"
	],
	"scripts": {
		"vscode:prepublish": "yarn clean && yarn build:production",
		"start": "tsc -watch -p ./",
		"build": "tsc -p ./",
		"build:production": "webpack --mode production",
		"clean": "git clean -xdf out *tsbuildinfo",
		"pretest": "yarn build && yarn lint",
		"test": "node ./out/test/runTest.js",
		"lint": "eslint src --ext ts",
		"deploy": "vsce publish --yarn"
	},
	"dependencies": {
		"@expo/xdl": "^57.9.1"
	},
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@expo/babel-preset-cli": "^0.2.11",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"babel-loader": "^8.1.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"shebang-loader": "^0.0.1",
		"typescript": "^3.8.3",
		"vsce": "^1.78.0",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	}
}
